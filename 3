#3c 
import pandas as pd 
import numpy as np 
c=pd.read_csv(r"data1.csv")
print(c.shape)
c.dropna(inplace=True)
print(c.shape)



#3d 
import pandas as pd 
import numpy as np
dia_data=pd.read_csv(r"data1.csv")
print("Before use of dropna()",dia_data.shape)
dia_data.dropna(axis=1,row='all',inplace=True)
print("After use of dropna()",dia_data.shape)
dia_data.head(5)




